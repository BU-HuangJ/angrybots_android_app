package base;

import Messaging.AIOServer.IOPair;
import Messaging.Response;

public class RPCEntry {
	private IOPair client;
	private Member member;
	public RPCEntry(IOPair client, Member member) {
		super();
		this.client = client;
		this.member = member;
	}
	public IOPair getClient() {
		return client;
	}
	public void setClient(IOPair client) {
		this.client = client;
	}
	public Member getMember() {
		return member;
	}
	public void setMember(Member member) {
		this.member = member;
	}
}
